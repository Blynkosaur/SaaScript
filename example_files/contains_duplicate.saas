# Contains Duplicate Problem
# Given an array of integers, return true if any value appears at least twice

mvp containsDuplicate(nums) {
    agentic (bootstrap i = 0; i < nums.arr; i = i + 1) {
        agentic (bootstrap j = i + 1; j < nums.arr; j = j + 1) {
            disrupt (nums[i] == nums[j]) {
                saas unicorn;
            }
        }
    }
    saas burnout;
}

leverage("Contains Duplicate Problem:");
leverage("");

# Test case 1: Has duplicates
bootstrap test1 = [1, 2, 3, 1];
bootstrap result1 = containsDuplicate(test1);
leverage("Test 1: [1, 2, 3, 1]");
leverage("Has duplicate: ");
leverage(result1);
leverage("Expected: true");
leverage("");

