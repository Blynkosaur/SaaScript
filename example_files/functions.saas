# Function definitions and calls
mvp greet(name) {
    leverage("Hello, ");
    leverage(name);
    leverage("!");
}

mvp add(a, b) {
    saas a + b;
}

mvp multiply(a, b) {
    saas a * b;
}

# Call functions
greet("SaaScript");
leverage("");
bootstrap result1 = add(5, 3);
leverage("5 + 3 = ");
leverage(result1);

bootstrap result2 = multiply(4, 7);
leverage("4 * 7 = ");
leverage(result2);
